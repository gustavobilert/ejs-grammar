{
  "EJS No Output": {
    "prefix": "ejs",
    "body": [
      "<% $1 %> $2"
    ],
    "description": "EJS No Output"
  },
  "EJS No Output (Whitespace handling)": {
    "prefix": "wejs",
    "body": [
      "<%_ $1 _%> $2"
    ],
    "description": "EJS No Output (Whitespace handling)"
  },
  "EJS Output Value": {
    "prefix": "ejsout",
    "body": [
      "<%= $1 %> $2"
    ],
    "description": "EJS outputs no value"
  },
  "EJS Output Escaped": {
    "prefix": "ejsesc",
    "body": [
      "<%- $1 %> $2"
    ],
    "description": "EJS outputs value"
  },
  "EJS Comment": {
    "prefix": "ejscom",
    "body": [
      "<%# $1 %> $2"
    ],
    "description": "EJS comment tag with no output"
  },
  "EJS Literal": {
    "prefix": "ejslit",
    "body": [
      "<%% $1 %> $2"
    ],
    "description": "EJS outputs a literal '<%'"
  },
  "EJS Include": {
    "prefix": "ejsinc",
    "body": [
      "<% include $1 %> $2"
    ],
    "description": "EJS include statement"
  },
  "EJS For Loop": {
    "prefix": "ejsfor",
    "body": [
      "<% for( let ${1:index} = 0; ${1:index} < ${2:array}.length; ${1:index}++ ) { %>",
      "$3",
      "<% } %>"
    ],
    "description": "EJS For Loop"
  },
  "EJS For Loop (Whitespace handling)": {
    "prefix": "wejsfor",
    "body": [
      "<%_ for( let ${1:index} = 0; ${1:index} < ${2:array}.length; ${1:index}++ ) { _%>",
      "$3",
      "<%_ } _%>"
    ],
    "description": "EJS For Loop (Whitespace handling)"
  },
  "EJS ForEach": {
    "prefix": "ejseach",
    "body": [
      "<% ${1:array}.forEach(${2:element} => { %>",
      " $3",
      "<% }) %>"
    ],
    "description": "EJS ForEach Loop"
  },
  "EJS ForEach (Whitespace handling)": {
    "prefix": "wejseach",
    "body": [
      "<%_ ${1:array}.forEach(${2:element} => { _%>",
      " $3",
      "<%_ }) _%>"
    ],
    "description": "EJS ForEach Loop (Whitespace handling)"
  },
  "EJS If Statement": {
    "prefix": "ejsif",
    "body": [
      "<% if (${1:condition}) { %>",
      " $2",
      "<% } %>"
    ],
    "description": "EJS if statement"
  },
  "EJS If Statement (Whitespace handling)": {
    "prefix": "wejsif",
    "body": [
      "<%_ if (${1:condition}) { _%>",
      " $2",
      "<%_ } _%>"
    ],
    "description": "EJS if statement (Whitespace handling)"
  },
  "EJS Else Statement": {
    "prefix": "ejselse",
    "body": [
      "<% } else { %>",
      " $1"
    ],
    "description": "EJS else statement"
  },
  "EJS Else Statement (Whitespace handling)": {
    "prefix": "wejselse",
    "body": [
      "<%_ } else { _%>",
      " $1"
    ],
    "description": "EJS else statement (Whitespace handling)"
  },
  "EJS Else If Statement": {
